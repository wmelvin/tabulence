{
    "manifest_version": 2,
    "name": "Tabulence",
    "description": "Shows the title and URL for all open tabs as text, Markdown, or list items.",
    "version": "0.1",
    "icons": {
        "48": "tabulence-icon.svg",
        "96": "tabulence-icon.svg"
    },      
    "permissions": ["tabs"],
    "browser_action": {
        "default_title": "Tabulence",
        "default_popup": "tabulence.html",
        "browser_style": false
    }
}